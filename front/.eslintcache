Auth2RedirectHandler.js",["139"],"import React from \"react\";\r\nimport { Redirect, useLocation } from \"react-router-dom\";\r\n\r\nconst OAuth2RedirectHander = () => {\r\n  const location = useLocation();\r\n\r\n  const getUrlParameter = (name) => {\r\n    name = name.replace(/[\\[]/, \"\\\\[\").replace(/[\\]]/, \"\\\\]\");\r\n    const regex = new RegExp(\"[\\\\?&]\" + name + \"=([^&#]*)\");\r\n\r\n    const result = regex.exec(location.search);\r\n    return result === null\r\n      ? \"\"\r\n      : decodeURIComponent(result[1].replace(/\\+/g, \" \"));\r\n  };\r\n\r\n  const token = getUrlParameter(\"token\");\r\n  const error = getUrlParameter(\"error\");\r\n\r\n  if (token) {\r\n    localStorage.setItem(\"accessToken\", token);\r\n    return (\r\n      <Redirect\r\n        to={{\r\n          pathname: \"/\",\r\n          state: { from: location },\r\n        }}\r\n      />\r\n    );\r\n  } else {\r\n    return (\r\n      <Redirect\r\n        to={{\r\n          pathname: \"/login\",\r\n          state: {\r\n            from: location,\r\n            error: error,\r\n          },\r\n        }}\r\n      />\r\n    );\r\n  }\r\n};\r\n\r\nexport default OAuth2RedirectHander;\r\n","D:\\GitHub\\open_date_course\\front\\src\\components\\course_search\\SearchCourse.js",["140"],"import React, { useEffect } from \"react\";\r\nimport { useSelector } from \"react-redux\";\r\nimport Course from \"./Course\";\r\nimport SearchBoxInSearchCourse from \"./SearchBoxInSearchCourse\";\r\nimport SearchResultBoxInSearchCourse from \"./SearchResultBoxInSearchCourse\";\r\n\r\nconst SearchCourse = () => {\r\n  const selectedMarker = useSelector((store) => store.marker);\r\n\r\n  useEffect(() => {\r\n    selectedMarker.forEach((marker) => {\r\n      marker.setMap(null);\r\n    });\r\n  }, []);\r\n  return (\r\n    <div>\r\n      <SearchBoxInSearchCourse />\r\n      <SearchResultBoxInSearchCourse />\r\n      <Course />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default SearchCourse;\r\n","